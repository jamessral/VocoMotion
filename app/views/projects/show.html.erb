<div class="container col-md-6 col-md-offset-4" id="show-container">
    <% if alert %>
        <div class="alert alert-success"><%= notice %></div>
    <% end %>
    <% if notice %>
        <div class="alert alert-warning"><%= alert %></div>
    <% end %>
    
    <div class="show-view">
        <p>
        <strong>Name:</strong>
        <%= @project.name %>
        </p>

        <p>
        <strong>Type:</strong>
        <%= @project.project_type %>
        </p>

        <p>
        <strong>Location:</strong>
        <%= @project.location %>
        </p>

        <p>
        <strong>Time:</strong>
        <%= @project.time %>
        </p>

        <p>
        <strong>Discipline:</strong>
        <%= @project.discipline %>
        </p>
    </div>

    <%= link_to 'Edit', edit_project_path(@project) %> |
    <%= link_to 'Back', projects_path %>
</div>
